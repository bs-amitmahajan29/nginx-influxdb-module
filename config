ngx_addon_name=ngx_http_influxdb_module
HEADERS_MORE_SRCS="$ngx_addon_dir/src/ngx_http_influxdb.c $ngx_addon_dir/src/ngx_http_influxdb_metric.c"

HEADERS_MORE_DEPS="$ngx_addon_dir/src/ngx_http_influxdb_metric.h"

if test -n "$ngx_module_link"; then
  ngx_module_type=HTTP
  ngx_module_name=$ngx_addon_name
  ngx_module_srcs="$HEADERS_MORE_SRCS"
  ngx_module_deps="$HEADERS_MORE_DEPS"
  . auto/module
else
  NGX_HTTP_MODULES="$NGX_HTTP_MODULES $ngx_addon_name"
  NGX_ADDON_SRCS="$NGX_ADDON_SRCS $HEADERS_MORE_SRCS"
  NGX_ADDON_DEPS="$NGX_ADDON_DEPS $HEADERS_MORE_DEPS"
fi
